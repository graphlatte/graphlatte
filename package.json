{
  "version": "0.0.0",
  "dependencies": {
    "@envelop/core": "^1.6.0",
    "@next/bundle-analyzer": "^12.0.3",
    "@prisma/client": "^3.4.1",
    "@tailwindcss/forms": "^0.3.4",
    "@tailwindcss/typography": "^0.4.1",
    "@tippyjs/react": "^4.2.6",
    "argon2": "^0.28.2",
    "clsx": "^1.1.1",
    "framer-motion": "^5.2.1",
    "graphql": "15.7.1",
    "graphql-helix": "^1.9.1",
    "next": "^12.0.3",
    "next-iron-session": "^4.2.0",
    "next-urql": "^3.2.1",
    "nexus": "^1.1.0",
    "nexus-prisma": "^0.34.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-icons": "^4.3.1",
    "urql": "^2.0.5",
    "zod": "^3.11.6"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "^2.2.2",
    "@graphql-codegen/near-operation-file-preset": "^2.2.1",
    "@graphql-codegen/typed-document-node": "^2.2.0",
    "@graphql-codegen/typescript": "^2.3.1",
    "@graphql-codegen/typescript-operations": "^2.2.0",
    "@graphql-typed-document-node/core": "^3.1.0",
    "@types/node": "^16.11.6",
    "@types/react": "^17.0.34",
    "autoprefixer": "^10.4.0",
    "eslint": "<8.0.0",
    "eslint-config-next": "^12.0.3",
    "husky": "^7.0.4",
    "prettier": "^2.4.1",
    "pretty-quick": "^3.1.1",
    "prisma": "^3.4.1",
    "tailwindcss": "^2.2.19",
    "typescript": "^4.4.4"
  },
  "license": "MIT",
  "prisma": {
    "schema": "src/graphql/database/schema.prisma"
  },
  "private": true,
  "scripts": {
    "analyze": "ANALYZE=true yarn build",
    "build": "next build",
    "db": "docker-compose up",
    "db:migrate": "prisma migrate dev",
    "db:push": "prisma db push",
    "db:studio": "prisma studio",
    "dev": "next",
    "generate": "prisma generate && yarn generate:nexus && yarn generate:documents",
    "generate:documents": "graphql-codegen --config graphql.config.js",
    "generate:env": "npx ts-node --skip-project --transpile-only src/library/generate.env.ts",
    "generate:nexus": "npx ts-node --skip-project --transpile-only -r tsconfig-paths/register src/graphql/schema --nexus-exit",
    "lint": "next lint",
    "prepare": "husky install && yarn prisma generate",
    "start": "next start"
  }
}
